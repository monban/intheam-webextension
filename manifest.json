{
    "manifest_version": 2,
    "name": "inthe.am integration",
    "description": "Create inthe.am tasks directly from your browser",
    "version": "0.1",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "options_ui": {
      "page": "options.html"
    },
    "permissions": [
      "https://inthe.am/*",
      "contextMenus",
      "storage"
    ]
}
